{
  "name": "vue-2-basic-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test": "vue-cli-service test:unit && vue-cli-service test:e2e --headless",
    "unit": "vue-cli-service test:unit --coverage",
    "test:e2e": "vue-cli-service test:e2e --mode e2e"
  },
  "dependencies": {
    "@vue/cli-plugin-babel": "4.1.1",
    "axios": "^0.27.2",
    "bootstrap": "4.1.1",
    "font-awesome": "^4.7.0",
    "vue": "^2.6.11",
    "vue-i18n": "^8.0.0",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.9.2",
    "@faker-js/faker": "^7.4.0",
    "@vue/cli-plugin-e2e-cypress": "^4.5.11",
    "@vue/cli-plugin-eslint": "4.1.1",
    "@vue/cli-plugin-unit-jest": "^4.2.2",
    "@vue/cli-service": "4.1.1",
    "@vue/test-utils": "^1.3.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^23.6.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.0.1",
    "vue-template-compiler": "^2.7.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "keywords": [
    "vuejs",
    "vue-router",
    "javascript",
    "i18n",
    "eslint",
    "skeleton",
    "starter",
    "webapp",
    "web",
    "app",
    "mvp",
    "frontend",
    "testing",
    "prettier",
    "cypress"
  ],
  "description": ""
}
